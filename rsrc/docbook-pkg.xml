   </rng>
   <rnc>
      <import-uri>http://docbook.org/schemas/docbookxi.rnc</import-uri>
      <file>rng/docbookxi.rnc</file>
   </rnc>
   <rng>
      <import-uri>http://docbook.org/schemas/docbookxi.rng</import-uri>
      <file>rng/docbookxi.rng</file>
   </rng>
   <xsd>
      <import-uri>http://docbook.org/schemas/docbook.xsd</import-uri>
      <file>xsd/docbook.xsd</file>
   </xsd>
   <xsd>
      <namespace>http://docbook.org/ns/docbook</namespace>
      <file>xsd/docbook.xsd</file>
   </xsd>
</package>
