<package xmlns="http://expath.org/ns/pkg"
         name="http://docbook.org/schemas"
         version="{@VERSION}"
         abbrev="docbook"
         spec="1.0">
   <title>Schemas for DocBook {@VERSION}</title>
   <home>http://docbook.org/</home>
   <nvdl>
      <import-uri>http://docbook.org/schemas/docbook.nvdl</import-uri>
      <file>docbook.nvdl</file>
   </nvdl>
   <dtd>
      <public-id>-//OASIS//DTD DocBook XML 5.0//EN</public-id>
      <system-id>http://docbook.org/schemas/docbook.dtd</system-id>
      <file>dtd/docbook.dtd</file>
